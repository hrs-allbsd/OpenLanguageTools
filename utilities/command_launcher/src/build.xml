<?xml version="1.0" encoding="UTF-8"?>
<project basedir="." default="about" name="Command Launcher">
    <target name="init">
        <tstamp/>
        <property name="jar-loc" value="/space/timf/transtech/utilities/jars"/>       
        <property name="class-path" value="../classes:../jars/alignimp.jar:${jar-loc}/oracle-jdbc.jar"/>
        <property name="build" value="../classes"/>

    </target>

    <target depends="importer" name="all"> 
    </target>

    <target depends="init" name="importer">
        <mkdir dir="${build}"/>
	<javac debug="true" classpath="${class-path}" destdir="${build}" excludes="**/SCCS/*, **/other/*" srcdir="."/>
    </target>

    <target depends="importer" name="jarfile">
	<jar basedir="${build}" jarfile="../importer.jar"/>
    </target>
 

    <target depends="init" name="clean">       
        <delete dir="${build}/com"/>
    </target>
</project>
